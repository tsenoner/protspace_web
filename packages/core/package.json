{
  "name": "@protspace/core",
  "version": "0.1.0",
  "description": "Core ProtSpace web components",
  "main": "dist/core.js",
  "module": "dist/core.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/core.mjs",
      "require": "./dist/core.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "test": "vitest",
    "test:ci": "vitest --run",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "protspace",
    "web-components",
    "visualization",
    "protein",
    "bioinformatics"
  ],
  "license": "MIT",
  "dependencies": {
    "@protspace/utils": "workspace:*",
    "apache-arrow": "^20.0.0",
    "d3": "^7.9.0",
    "hyparquet": "^1.17.1",
    "lit": "^3.3.0"
  },
  "devDependencies": {
    "@types/d3": "^7.4.3",
    "@typescript-eslint/eslint-plugin": "^8.46.0",
    "@typescript-eslint/parser": "^8.46.0",
    "eslint": "^9.27.0",
    "jsdom": "^27.4.0",
    "typescript": "^5.8.3",
    "vite": "^6.3.5",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^4.0.16"
  },
  "peerDependencies": {
    "@protspace/utils": "workspace:*"
  }
}
